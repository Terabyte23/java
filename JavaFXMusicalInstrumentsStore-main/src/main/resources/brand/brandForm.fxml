<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vbBrandFormRoot" prefHeight="236.0" prefWidth="602.0" stylesheets="@../css/appStyle.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.MusicalInstrumentStoreFX.controller.BrandFormController">
    <children>
        <Label alignment="CENTER" prefHeight="81.0" prefWidth="562.0" text="Новый бренд">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <HBox prefHeight="3.0" prefWidth="562.0" />
        <HBox layoutX="10.0" layoutY="99.0" prefHeight="28.0" prefWidth="600.0">
         <children>
                <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="161.0" text="Название бренда">
                    <HBox.margin>
                        <Insets right="10.0" />
                    </HBox.margin>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
                </Label>
                <TextField fx:id="tfFirstName" prefHeight="25.0" prefWidth="308.0" />
         </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="127.0" prefHeight="58.0" prefWidth="600.0" spacing="5.0">
            <children>
                <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="154.0">
                    <HBox.margin>
                        <Insets right="10.0" />
                    </HBox.margin>
                </Label>
                <Button mnemonicParsing="false" onAction="#create" prefHeight="25.0" prefWidth="125.0" text="Добавить">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Button>
                <Button mnemonicParsing="false" onAction="#goToMainForm" prefHeight="25.0" prefWidth="146.0" text="Возврат на главную">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Button>
            </children>
        </HBox>
    </children>
</VBox>
